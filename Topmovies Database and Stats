INSERT INTO topmovies(Rank,Title,Studio,Worldwide,Domestic,DomesticPct,Overseas,OverseasPct,Year) VALUES (100,'Tangled','BV',591.80,200.80,0.34,391.00,0.66,2010);/* Put your data in here and query it! */

SELECT MAX(Domestic) FROM topmovies;

SELECT AVG(Year) FROM topmovies;

SELECT MIN(Overseas) FROM topmovies;

SELECT Title FROM topmovies
GROUP BY Title
HAVING Studio = "P/DW";

SELECT Studio FROM topmovies
GROUP BY Studio
HAVING Worldwide > 500.0;

SELECT Title FROM topmovies
GROUP BY Studio
HAVING Worldwide > 500.0;

SELECT Title, Year, 
    CASE
        WHEN Title = "Avatar" THEN "Action, Adventure, Fantasy, Science Fiction, Mystery."
        WHEN Title = "Titanic" THEN "Romance, Disaster, Drama, Historical Drama."
        WHEN Title = "Marvel''s The Avengers" THEN "Action, Adventure, Superhero, Science Fiction, Fantasy."
        WHEN Title = "Harry Potter and the Deathly Hallows Part 2" THEN "Adventure, Drama, Fantasy, Mystery, Narrative."
        WHEN Title = "Furious 7" THEN "Action, Crime Film, Adventure, Thriller, Mystery."
        WHEN Title = "Frozen" THEN "Children's Film, Musical, Comedy, Adventure, Fantasy, Animated Cartoon, Comedy Music."
        WHEN Title = "Iron Man 3" THEN "Action, Comedy, Superhero, Science Fiction, Adventure."
        WHEN Title = "Transformers: Dark of the Moon" THEN "Action, Science Fiction, Adventure."
        WHEN Title = "The Lord of the Rings: The Return of the King" THEN "Action, Fantasy, Adventure, Drama."
        WHEN Title = "Skyfall" THEN "Action, Spy, Thriller, Adventure, Mystery, Crime Fiction."
        WHEN Title = "Transformers: Age of Extinction" THEN "Action, Science Fiction, Adventure, Fantasy."
        WHEN Title = "The Dark Knight Rises" THEN "Action, Superhero, Drama, Thriller, Crime Fiction."
        WHEN Title = "Pirates of the Caribbean: Dead Man's Chest" THEN "Action, Comedy, Adventure, Fantasy, Swashbuckler."
        WHEN Title = "Toy Story 3" THEN "Comedy, Adventure, Children's Film, Fantasy, Comedy Drama, Coloring Cartoon."
        WHEN Title = "Pirates of the Caribbean: On Stranger Tides" THEN "Action, Adventure, Fantasy, Swashbuckler."
        WHEN Title = "Jurassic Park" THEN "Action, Science Fiction, Adventure, Fantasy, Thriller, Mystery."
        WHEN Title = "Star Wars: Episode I - The Phantom Menace" THEN "Action, Science Fiction, Children's Film, Adventure, Fantasy, Space Opera."
        WHEN Title = "Alice in Wonderland (2010)" THEN "Adventure, Fantasy, Children's Film, Science Fiction."
        WHEN Title = "The Hobbit: An Unexpected Journey" THEN "Fantasy, Action, Adventure, Children's Film, Science Fiction, High Fantasy."
        WHEN Title = "The Dark Knight" THEN "Action, Superhero, Thriller, Crime Film, Adventure, Noir, Mystery, Drama, Neo-noir."
        WHEN Title = "The Lion King" THEN "Children's Film, Musical, Adventure, Drama, Coloring Cartoon, Musical Drama."
        WHEN Title = "Harry Potter and the Sorcerer''s Stone" THEN "Fantasy, Adventure, Children's Film, Narrative."
        WHEN Title = "Despicable Me 2" THEN "Children's Film, Comedy, Adventure, Science Fiction, Family Film, Coloring Cartoon, Crime Fiction."
        WHEN Title = "Pirates of the Caribbean: At World's End" THEN "Action, Adventure, Comedy, Fantasy, Swashbuckler."
        WHEN Title = "The Hobbit: The Desolation of Smaug" THEN "Fantasy, Adventure, Drama, High Fantasy."
        WHEN Title = "Harry Potter and the Deathly Hallows Part 1" THEN "Action, Fantasy, Children's Film, Adventure, Mystery, Adaptation, Narrative."
        WHEN Title = "The Hobbit: The Battle of the Five Armies" THEN "Fantasy, Adventure, High Fantasy."
        WHEN Title = "Finding Nemo" THEN "Children's Film, Comedy, Adventure, Coloring Cartoon."
        WHEN Title = "Harry Potter and the Order of the Phoenix" THEN "Action, Fantasy, Children's Film, Adventure, Drama, Mystery."
        WHEN Title = "Harry Potter and the Half-Blood Prince" THEN "Action, Romance, Children's Film, Fantasy, Adventure, Mystery, Narrative."
        WHEN Title = "The Lord of the Rings: The Two Towers" THEN "Action, Fantasy, Adventure, Drama."
        WHEN Title = "Shrek 2" THEN "Comedy, Children's Film, Adventure, Fantasy, Coloring Cartoon."
        WHEN Title = "Harry Potter and the Goblet of Fire" THEN "Children's Film, Fantasy, Adventure, Thriller, Mystery."
        WHEN Title = "Spider-Man 3" THEN "Action, Superhero, Science Fiction, Adventure, Thriller."
        WHEN Title = "Ice Age: Dawn of the Dinosaurs" THEN "Action, Romance, Comedy, Adventure, Science Fiction, Coloring Cartoon."
        WHEN Title = "Harry Potter and the Chamber of Secrets" THEN "Children's Film, Fantasy, Adventure, Mystery."
        WHEN Title = "Ice Age: Continental Drift" THEN "Children's Film, Comedy, Fantasy, Coloring Cartoon."
        WHEN Title = "The Lord of the Rings: The Fellowship of the Ring" THEN "Action, Fantasy, Adventure."
        WHEN Title = "The Hunger Games: Catching Fire" THEN "Action, Science Fiction, Adventure, Thriller."
        WHEN Title = "Star Wars: Episode III - Revenge of the Sith" THEN "Action, Science Fiction, Adventure, Fantasy, Space Opera."
        WHEN Title = "Transformers: Revenge of the Fallen" THEN "Action, Science Fiction, Adventure, Thriller."
        WHEN Title = "The Twilight Saga: Breaking Dawn Part 2" THEN "Romance, Fantasy, Adventure, Teen, Drama."
        WHEN Title = "Inception" THEN "Action, Science Fiction, Thriller, Adventure, Heist, Drama."
        WHEN Title = "Spider-Man" THEN "Action, Superhero, Adventure, Fantasy, Science Fiction."
        WHEN Title = "Independence Day" THEN "Action, Disaster, Action Adventure, Science Fiction, Thriller."
        WHEN Title = "Shrek the Third" THEN "Children's Film, Comedy, Adventure, Fantasy, Coloring Cartoon."
        WHEN Title = "Harry Potter and the Prisoner of Azkaban" THEN "Children's Film, Fantasy, Adventure, Mystery, Family Film, Narrative."
        WHEN Title = "E.T.: The Extra-Terrestrial" THEN "Children's Film, Science Fiction, Action, Fantasy, Adventure, Drama."
        WHEN Title = "Fast & Furious 6" THEN "Action, Mystery, Thriller."
        WHEN Title = "Spider-Man 2" THEN "Action, Superhero, Science Fiction, Adventure."
        WHEN Title = "Star Wars" THEN "Science Fiction, Action, Fantasy, Adventure, Space Opera."
        WHEN Title = "Indiana Jones and the Kingdom of the Crystal Skull" THEN "Action, Adventure, Children's Film, Fantasy, Thriller."
        WHEN Title = "Guardians of the Galaxy" THEN "Action, Comedy, Superhero, Science Fiction, Adventure, Fantasy."
        WHEN Title = "2012" THEN "Disaster, Action, Science Fiction, Thriller, Adventure, Drama."
        WHEN Title = "Maleficent" THEN "Action, Fantasy, Children's Film, Romance, Adventure."
        WHEN Title = "The Da Vinci Code" THEN "Thriller, Mystery, Crime Thriller."
        WHEN Title = "The Amazing Spider-Man" THEN "Action, Superhero, Science Fiction, Adventure, Fantasy, Action Adventure, Thriller, Drama."
        WHEN Title = "Shrek Forever After" THEN "Comdey, Children's Film, Adventure, Fantasy, Comedy Drama, Coloring Cartoon."
        WHEN Title = "The Hunger Games: Mockingjay - Part 1" THEN "Science Fiction, War, Adventure, Fantasy, Thriller, Drama."
        WHEN Title = "X-Men: Days of Future Past" THEN "Action, Superhero, Science Fiction, Adventure, Fantasy, Thriller."
        WHEN Title = "Madagascar 3: Europe's Most Wanted" THEN "Children's Film, Comedy, Adventure, Coloring Cartoon."
        WHEN Title = "The Chronicles of Narnia: The Lion, the Witch and the Wardrobe" THEN "Fantasy, Adventure, Children's Film."
        WHEN Title = "Monsters University" THEN "Comedy, Children's Film, Adventure, Fantasy, Coloring Cartoon."
        WHEN Title = "The Matrix Reloaded" THEN "Action, Science Fiction, Cyberpunk, Adventure, Thriller, Apocalytpic Post-apocalyptic Fiction."
        WHEN Title = "Up" THEN "Adventure, Children's Film, Comedy, Fantasy, Comedy Drama, Drama, Buddy, Coloring Cartoon."
        WHEN Title = "Gravity" THEN "Science Fiction, Thriller, Disaster, Drama, Mystery."
        WHEN Title = "Captain America: The Winter Soldier" THEN "Action, Superhero, Science Fiction, Thriller, Adventure, Spy." 
        WHEN Title = "The Twilight Saga: Breaking Dawn Part 1" THEN "Drama, Melodrama, Romance, Teen, Fantasy, Adventure, Thriller." 
        WHEN Title = "The Twilight Saga: New Moon" THEN "Romance, Melodrama, Fantasy, Adventure, Drama."
        WHEN Title = "Transformers" THEN "Action, Science Fiction, Adventure, Thriller."
        WHEN Title = "The Amazing Spider-Man 2" THEN "Action, Superhero, Science Fiction, Adventure, Fantasy."
        WHEN Title = "Dawn of the Planet of the Apes" THEN "Action, Science Fiction, Adventure, Thriller, Drama."
        WHEN Title = "The Twilight Saga: Eclipse" THEN "Horror, Action, Romance, Melodrama, Thriller, Fantasy, Adventure, Teen, Drama."
        WHEN Title = "Mission: Impossible - Ghost Protocol" THEN "Action, Spy, Adventure, Thriller."
        WHEN Title = "The Hunger Games" THEN "Action, Science Fiction, Adventure, Thriller, Fantasy, Mystery."
        WHEN Title = "Forrest Gump" THEN "Comedy, Romance, Romantic Comedy, Comedy Drama, Drama, Magical Reralism."
        WHEN Title = "The Sixth Sense" THEN "Horror, Thriller, Mystery, Psychological Thriller, Suspense, Drama, Supernatural."
        WHEN Title = "Interstellar" THEN "Science Fiction, Adventure, Mystery, Drama."
        WHEN Title = "Man of Steel" THEN "Action, Superhero, Science Fiction, Adventure, Fantasy."
        WHEN Title = "Kung Fu Panda 2" THEN "Action, LGBT, Children's Film, Comedy, Marital Arts, Adventure, Wuxia, Comedy Drama, Coloring Cartoon."
        WHEN Title = "Ice Age: The Meltdown" THEN "Action, Comedy, Adventure, Children's Film, Fantasy, Coloring Cartoon."
        WHEN Title = "Pirates of the Caribbean: The Curse of the Black Pearl" THEN "Action, Adventure, Comedy, Fantasy, Swashbuckler."
        WHEN Title = "Big Hero 6" THEN "Action, Comedy, Adventure, Superhero, Science Fiction, Coloring Cartoon."
        WHEN Title = "Star Wars: Episode II - Attack of the Clones" THEN "Action, Romance, Children's Film, Science Fiction, Space Opera, Adventure, Fantasy."
        WHEN Title = "Thor: The Dark World" THEN "Action, Superhero, Science Fiction, Fantasy, Adventure."
        WHEN Title = "Kung Fu Panda" THEN "Children's Film, Action, Comedy, Adventure, Martial Arts, Wuxia, Coloring Cartoon."
        WHEN Title = "The Incredibles" THEN "Children's Film, Action, Superhero, Comedy, Adventure, Coloring Cartoon."
        WHEN Title = "Fast Five" THEN "Action, Heist, Crime."
        WHEN Title = "Hancock" THEN "Action, Superhero, Comedy, Drama, Science Fiction, Adventure, Fantasy, Crime Film, Comedy Drama."
        WHEN Title = "MIB 3" THEN "Action, Comedy, Science Fiction, Children's Film, Science Fiction Comedy, Adventure, Buddy."
        WHEN Title = "Iron Man 2" THEN "Action, Superhero, Science Fiction, Adventure."
        WHEN Title = "Ratatouille" THEN "Children's Film, Comedy, Adventure, Fantasy, Drama, Coming-of-age Story, Coloring Cartoon."
        WHEN Title = "How to Train Your Dragon 2" THEN "Action, Comedy, Children's Film, Adventure, Fantasy, Coloring Cartoon."
        WHEN Title = "The Lost World: Jurassic Park" THEN "Action, Adventure, Science Fiction, Thriller, Adaptation."
        WHEN Title = "The Passion of the Christ" THEN "Horror, Religious, Drama, Costume Drama, Indie Film, Historical Fiction."
        WHEN Title = "Mamma Mia!" THEN "Musical, Romance, Comedy, Romantic Comedy, Jukebox Musical."
        WHEN Title = "Life of Pi" THEN "Adventure, Survival, Fantasy, Magical Realism, Drama, Action."
        WHEN Title = "Madagascar: Escape 2 Africa" THEN "Action, Comedy, Children's Film, Adventure, Coloring Cartoon."
        WHEN Title = "Casino Royale" THEN "Action, Spy, Adventure, Thriller, Mystery, Crime Fiction."
        WHEN Title = "Tangled" THEN "Romance, Children's Film, Musical, Comedy, Adventure, Fantasy, Coloring Cartoon, Comedy Music."
        END AS Film_Genre
FROM topmovies
GROUP BY Film_Genre;

SELECT Title From topmovies;

SELECT Title, Year From topmovies
WHERE Year > '2003' AND Year < '2010'
GROUP BY Year;

SELECT Title, Year, Studio From topmovies
WHERE Year > '2010' OR Year < '2015'
ORDER By Studio, Year; 

